{
  "source": "./lib",
  "destination": "./docs",
  "plugins": [{
      "name": "esdoc-standard-plugin",
      "option": {
        "lint": {"enable": true},
        "coverage": {"enable": true},
        "accessor": {"access": ["public", "protected"], "autoPrivate": true},
        "typeInference": {"enable": true},
        "test": {
          "source": "./spec/",
          "interfaces": ["describe", "it"],
          "includes": [".*-spec\\.js$"]
        }
      }
    },
    {
      "name": "esdoc-jsx-plugin"
    }
  ]
}
