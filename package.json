{
  "name": "pkg-groups",
  "main": "./lib/pkg-groups",
  "version": "0.0.2",
  "description": "Define groups of packages, and enable/disable them.",
  "keywords": [
    "package",
    "configuration"
  ],
  "activationCommands": {
    "atom-workspace": "pkg-groups:toggle"
  },
  "repository": "https://github.com/alflanagan/atom-pkg-groups",
  "license": "MIT",
  "engines": {
    "atom": ">=1.19.0 <2.0.0"
  },
  "dependencies": {
    "etch": "^0.12.6",
    "immutable": "^3.8.1",
    "log4js": "^2.3.3"
  },
  "deserializers": {
    "PkgGroupsView": "deserializePkgGroupsView",
    "PkgGroupsModel": "deserializePkgGroupsModel"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "standard": "*"
  },
  "scripts": {
    "test": "standard && apm test",
    "make-tar": "rm -f pkg-groups.tar; tar cf pkg-groups.tar $(find . -name .git -prune -o -name node_modules -prune -o -type f -print)"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "atom"
    ]
  }
}
